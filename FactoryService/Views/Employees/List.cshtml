@model IEnumerable<FactoryService.Models.Employee>
<table>
    <tr><th>Id</th><th>Surname</th><th>Firstname</th><th>Patronymic</th><th>EmploymentDate</th><th>Position</th><th>Company</th></tr>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.Id</td>
            <td>@employee.Surname</td>
            <td>@employee.Firstname</td>
            <td>@employee.Patronymic</td>
            <td>@employee.EmploymentDate</td>
            <td>@employee.Position</td>
            <td>@employee.Company</td>
            <td>
                <a asp-controller="employees" asp-action="edit" asp-route-id="@employee.Id">Изменить</a>
                <a asp-controller="employees" asp-action="delete" asp-route-id="@employee.Id">Удалить</a>
            </td>
        </tr>
    }
</table>
<a asp-controller="employees" asp-action="create">Добавить</a>